<?xml version="1.0" encoding="UTF-8"?>

<web-app xmlns="http://xmlns.jcp.org/xml/ns/javaee"
	xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
	xsi:schemaLocation="http://xmlns.jcp.org/xml/ns/javaee 
         http://xmlns.jcp.org/xml/ns/javaee/web-app_3_1.xsd"
	version="3.1">

	<servlet>
		<servlet-name>loginAdmin</servlet-name>
		<servlet-class>com.servlets.loginAdminServlet</servlet-class>

	</servlet>
	<servlet-mapping>
		<servlet-name>loginAdmin</servlet-name>
		<url-pattern>/log</url-pattern>
	</servlet-mapping>

	<servlet>
		<servlet-name>Staff-insert</servlet-name>
		<servlet-class>com.servlets.Staffinsert</servlet-class>
	</servlet>

	<servlet-mapping>
		<servlet-name>Staff-insert</servlet-name>
		<url-pattern>/insert</url-pattern>
	</servlet-mapping>

	<servlet>
		<servlet-name>staff-delete</servlet-name>
		<servlet-class>com.servlets.deletestaff</servlet-class>
	</servlet>

	<servlet-mapping>
		<servlet-name>staff-delete</servlet-name>
		<url-pattern>/delete</url-pattern>
	</servlet-mapping>

	<servlet>
		<servlet-name>staff-servlet</servlet-name>
		<servlet-class>com.servlets.StaffServlet</servlet-class>
	</servlet>

	<servlet-mapping>
		<servlet-name>staff-servlet</servlet-name>
		<url-pattern>/list</url-pattern>
	</servlet-mapping>
	
	<servlet-mapping>
		<servlet-name>staff-servlet</servlet-name>
		<url-pattern>/edit</url-pattern>
	</servlet-mapping>
	
	<servlet-mapping>
		<servlet-name>staff-servlet</servlet-name>
		<url-pattern>/update</url-pattern>
	</servlet-mapping>

</web-app>
         